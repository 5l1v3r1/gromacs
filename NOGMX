# remove all environment variables set by GMXRC

if ( ! $?GMXHOME ) then
  if ( "$1" != "-quiet" ) echo "no GROMACS environment"
  exit
endif

if ($?GMXLIB) then
  if ($?LD_LIBRARY_PATH) then
    if ($LD_LIBRARY_PATH == $GMXHOME/lib/\$CPU) then
      unsetenv LD_LIBRARY_PATH
    endif
    if ( "$1" != "-quiet" ) echo "You might want to remove the gmx path from "\$"LD_LIBRARY_PATH"
  endif
  if ( "$1" != "-quiet" ) echo "unsetenv GMXLIB ($GMXLIB)"
  unsetenv GMXLIB 
endif

if ($?GMXMAN) then
  if ($MANPATH == $GMXMAN) then
    unsetenv MANPATH
  endif
  if ($?MANPATH) then
    if ( "$1" != "-quiet" ) echo "You might want to remove $GMXMAN ("\$"GMXMAN) from "\$"MANPATH"
  endif
  if ( "$1" != "-quiet" ) echo "unsetenv GMXMAN ($GMXMAN)"
  unsetenv GMXMAN
endif

if ($?GMXHOME) then
  if ( "$1" != "-quiet" ) echo "unsetenv GMXHOME ($GMXHOME)"
  unsetenv GMXHOME
endif
if ($?VER) then
  if ( "$1" != "-quiet" ) echo "unsetenv VER ($VER)"
  unsetenv VER
endif
if ($?CPUGMX) then
  if ( "$1" != "-quiet" ) echo "unsetenv CPU ($CPU)"
  unsetenv CPU
endif
if ($?GMXROOT) then
  if ( "$1" != "-quiet" ) echo "unsetenv GMXROOT ($GMXROOT)"
  unsetenv GMXROOT
endif
if ($?GMXBIN) then
  foreach i ( $path )
    if ( ( "$i" != "$GMXBIN" ) && ( "$i/" != "$GMXBIN" ) ) then
      if ( ! $?TEMPPATH ) then
	setenv TEMPPATH $i
      else
	setenv TEMPPATH "$TEMPPATH":"$i"
      endif
    endif
  end
  setenv PATH $TEMPPATH
  unsetenv TEMPPATH
  if ( "$1" != "-quiet" ) echo Removed $GMXBIN from path
  if ( "$1" != "-quiet" ) echo "unsetenv GMXBIN ($GMXBIN)"
  unsetenv GMXBIN
endif
if ($?GMXFONT) then
  if ( "$1" != "-quiet" ) echo "unsetenv GMXFONT ($GMXFONT)"
  unsetenv GMXFONT
endif
if ($?AMBBIN) then
  if ( "$1" != "-quiet" ) echo "unsetenv AMBBIN ($AMBBIN)"
  unsetenv AMBBIN
endif
if ($?IAMCOOL) then
  if ( "$1" != "-quiet" ) echo "unsetenv IAMCOOL ($IAMCOOL)"
  unsetenv IAMCOOL
endif

#last line
